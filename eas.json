{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "betaAPK": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": false,
      "env": {
        "APP_VARIANT": "beta"
      },
      "releaseChannel": "beta"
    },
    "betaAAB": {
      "android": {
        "buildType": "app-bundle"
      },
      "developmentClient": false,
      "env": {
        "APP_VARIANT": "beta"
      },
      "releaseChannel": "beta"
    },
    "development": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "APP_VARIANT": "development"
      },
      "releaseChannel": "development"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "releaseChannel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}